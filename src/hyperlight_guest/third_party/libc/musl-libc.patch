-		size_t j = 0;
-		for (size_t i=abbrevs-types; i; i-=6) {
-			if (!types[i-6+4]) j = i-6;
-		}
-		if (local) off = (int32_t)zi_read32(types + j);
-		/* If t is before first transition, use the above-found type
-		 * and the index-zero (after transition) type as the alt. */
-		if (t - off < (int64_t)x) {
-			if (alt) *alt = index[0];
-			return j/6;
-		}
-	}
