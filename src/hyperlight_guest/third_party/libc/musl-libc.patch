-static ssize_t read_loop(int fd, void *p, size_t n)
-{
-	for (size_t i=0; i<n; ) {
-		ssize_t l = read(fd, (char *)p+i, n-i);
-		if (l<0) {
-			if (errno==EINTR) continue;
-			else return -1;
-		}
-		if (l==0) return i;
-		i += l;
-	}
-	return n;
-}
