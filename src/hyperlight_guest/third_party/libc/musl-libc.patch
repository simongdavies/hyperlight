-	for (i=0; dynv[i]; i+=2) {
-		void *p = (void *)(base + dynv[i+1]);
-		switch(dynv[i]) {
-		case DT_STRTAB: strings = p; break;
-		case DT_SYMTAB: syms = p; break;
